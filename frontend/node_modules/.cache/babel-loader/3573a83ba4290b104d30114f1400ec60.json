AAAA,WAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,8BAAA;AACA,OATA;AAUA;AAxBA;AAXA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <section class=\"hero is-fullheight\">\r\n      <div class=\"hero-body\">\r\n        <div class=\"columns\">\r\n          <div class=\"column is-6 custom-center\">\r\n            <img src=\"../assets/logo.svg\" alt=\"Logo\" />\r\n            <h1 class=\"title is-4\">Seu gerenciador de contatos r√°pidos</h1>\r\n\r\n            <div class=\"card login\">\r\n              <div class=\"card-content\">\r\n                <form>\r\n                  <div class=\"field\">\r\n                    <p class=\"control has-icons-left has-icons-right\">\r\n                      <input\r\n                        name=\"email\"\r\n                        class=\"input\"\r\n                        v-model=\"form.email\"\r\n                        type=\"email\"\r\n                        placeholder=\"Seu email\"\r\n                      />\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"fas fa-envelope\"></i>\r\n                      </span>\r\n                      <span class=\"icon is-small is-right\">\r\n                        <i class=\"fas fa-check\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\">\r\n                    <p class=\"control has-icons-left\">\r\n                      <input\r\n                        name=\"password\"\r\n                        class=\"input\"\r\n                        v-model=\"form.password\"\r\n                        type=\"password\"\r\n                        placeholder=\"Sua senha\"\r\n                      />\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"fas fa-lock\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n\r\n                  <article v-if=\"alertError\" class=\"message is-danger\">\r\n                    <div class=\"message-body\">{{alertError}}</div>\r\n                  </article>\r\n\r\n                  <div class=\"field\">\r\n                    <p class=\"control\">\r\n                      <button id=\"signIn\" type=\"button\" @click=\"login()\" class=\"button is-success\">Entrar</button>\r\n                    </p>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"column is-6\">\r\n            <img src=\"../assets/landing.svg\" alt=\"Marca\" width=\"100%\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      alertError: \"\",\r\n      form: {\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      //console.log(this.form)\r\n      \r\n      if (this.form.email === '') {\r\n        this.alertError = 'Oops. Por favor informe o seu email'\r\n        return null\r\n      }\r\n        \r\n      if (this.form.password === '') {\r\n        this.alertError = 'Oops. Por favor informe sua senha'\r\n        return null\r\n      }\r\n        \r\n      window.axios\r\n        .post(\"/session\", this.form)\r\n        .then(async (res) => {\r\n          const resp = await res.data;\r\n          localStorage.setItem(\"user_token\", resp.user_token);\r\n          this.$router.push(\"/dashboard\");\r\n        })\r\n        .catch((error) => {\r\n          this.alertError = 'Email e/ou senha incorretos!'\r\n        })\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login {\r\n  width: 400px;\r\n  border-radius: 10px;\r\n}\r\n.custom-center {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  align-content: center;\r\n  align-items: center;\r\n}\r\n</style>\r\n"]