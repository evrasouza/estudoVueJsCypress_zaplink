      email: \"\",\r\n        password: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      //console.log(this.form)\r\n      \r\n      if (this.form.email === '') {\r\n        this.alertError = 'Oops. Por favor informe o seu email'\r\n        return null\r\n      }\r\n        \r\n      if (this.form.password === '') {\r\n        this.alertError = 'Oops. Por favor informe sua senha'\r\n        return null\r\n      }\r\n        \r\n      window.axios\r\n        .post(\"/session\", this.form)\r\n        .then(async (res) => {\r\n          const resp = await res.data;\r\n          localStorage.setItem(\"user_token\", resp.user_token);\r\n          this.$router.push(\"/dashboard\");\r\n        })\r\n        .catch((error) => {\r\n          this.alertError = 'Email e/ou senha incorretos!'\r\n        })\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login {\r\n  width: 400px;\r\n  border-radius: 10px;\r\n}\r\n.custom-center {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  align-content: center;\r\n  align-items: center;\r\n}\r\n</style>\r\n"]}]}